{
  "name": "Google PageSpeed Checker",
  "version": "1.0.0",
  "description": "Display pagespeed of the website from browser tab",
  "permissions": [
    "tabs"
  ],
  "manifest_version": 3,
  "author": "Arif Khan",
  "icons": {
    "16": "icons/gpsc_icon_16.png",
    "32": "icons/gpsc_icon_32.png",
    "48": "icons/gpsc_icon_48.png",
    "64": "icons/gpsc_icon_64.png",
    "128": "icons/gpsc_icon_128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Google PageSpeed Checker"
  }
}